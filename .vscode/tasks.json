{
    "version": "2.0.0",
    "tasks": [  
        {
            "label": "Maven UPom Builder",
            "detail": "build pom and deploy in server",
            "type": "shell",
            "command": "${workspaceFolder}/scriptBuilderWebService.ps1",
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            },
            "args": [
                "${workspaceFolder}",
                "source"
            ]
        }
    ]
}